public class Main{    
    static int factorial(int n){
        if(n==0||n==1){
            return 1;
        }
        return n* factorial(n-1);
    }
    static int sum(int n){
        if(n==0){
            return 0;
        }
        return n+sum(n-1);
    }
    public static void main(String[] main){
        System.out.println(factroial(5));
        System.out.println(sum(5));
    }
}
