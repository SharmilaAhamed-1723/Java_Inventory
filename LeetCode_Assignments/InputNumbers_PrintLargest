public class Main{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int i=1, max=0;
        while(i>0){
            System.out.print("Enter number "+i+" =");
            int n=sc.nextInt();
            if(n==0){
                break;
            }
            if(n>max){
               max=n;
            }
        }
        System.out.println(max);
    }
}
