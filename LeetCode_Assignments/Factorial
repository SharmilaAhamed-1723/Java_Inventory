 import java.util.Scanner;
 public class Main{
   static int factorial(int n){
        int c=1;
        int factorial =1;
        if(n==0){
            return 1;
        }
        while(c<=n){
            factorial*=c;
            c++;
        }
        return factorial;
    }
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter a number");
        int num= sc.nextInt();
        System.out.println(factorial(num));
    }
}
