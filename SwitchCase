import java.util.Scanner;
public class SwitchDemo {
    public static void main(String[] args) {
         Scanner sc=new Scanner(System.in);
         System.out.print("Enter your location: ");
         String loc=sc.next();
         System.out.print("Enter the destination: ");
         String des=sc.next();
         switch(des){
             case "street"->System.out.println("Go by walk");
             case "town"->System.out.println("Go by bus");
             case "city"->System.out.println("Go by car");
             case "state"->System.out.println("Go by train");
             case "country"->System.out.println("Go by plane");
             default->System.out.println("Enter a valid place");
                 
         }
    }
}
