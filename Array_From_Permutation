public class Main{
    static int[] Array_Of_permutation(int[] arr){
        int[] ans=new int[arr.length];
        for(int i=0; i<arr.length; i++){
            ans[i] = arr[arr[i]];   
        }
        return ans;
     }
       
       
    
    public static void main(String[] args){
        int[] arr={0,2,1,5,3,4};
        System.out.println(Arrays.toString(Array_Of_permutation(arr)));
    }
}
