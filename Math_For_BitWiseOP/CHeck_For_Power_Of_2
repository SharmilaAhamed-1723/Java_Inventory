import java.util.Scanner;
public class Main{   
    static boolean No_powerOf_2(int n){
        if (n==0){
           return false;
        }
    return (n&(n-1))==0;
    }    
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int n= sc.nextInt();
        System.out.println( n+ " is power of 2: "+No_powerOf_2(n));    
    }
}
